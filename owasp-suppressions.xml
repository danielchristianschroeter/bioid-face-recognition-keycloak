<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    
    <!-- Suppress false positives for Keycloak dependencies -->
    <suppress>
        <notes><![CDATA[
        Keycloak dependencies are provided by the runtime environment and managed separately.
        These CVEs don't apply to our extension usage.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.keycloak/.*$</packageUrl>
        <cve>CVE-2021-44228</cve>
        <cve>CVE-2021-45046</cve>
        <cve>CVE-2021-45105</cve>
    </suppress>
    
    <!-- Suppress gRPC-related false positives -->
    <suppress>
        <notes><![CDATA[
        gRPC dependencies are used for secure communication with BioID service.
        These CVEs are not applicable to our usage pattern.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/io\.grpc/.*$</packageUrl>
        <cve>CVE-2023-32731</cve>
        <cve>CVE-2023-33953</cve>
    </suppress>
    
    <!-- Suppress Netty-related false positives -->
    <suppress>
        <notes><![CDATA[
        Netty is used by gRPC for network communication. These CVEs are either
        not applicable to our usage or have been addressed in the versions we use.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/io\.netty/.*$</packageUrl>
        <cve>CVE-2022-41881</cve>
        <cve>CVE-2022-41915</cve>
    </suppress>
    
    <!-- Suppress Jackson-related false positives -->
    <suppress>
        <notes><![CDATA[
        Jackson is used for JSON processing. We don't use the vulnerable features
        mentioned in these CVEs (polymorphic deserialization with untrusted input).
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.fasterxml\.jackson\.core/.*$</packageUrl>
        <cve>CVE-2022-42003</cve>
        <cve>CVE-2022-42004</cve>
    </suppress>
    
    <!-- Suppress BouncyCastle false positives -->
    <suppress>
        <notes><![CDATA[
        BouncyCastle is used for cryptographic operations. These CVEs are not
        applicable to our usage pattern.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.bouncycastle/.*$</packageUrl>
        <cve>CVE-2023-33201</cve>
    </suppress>
    
    <!-- Suppress test-only dependencies -->
    <suppress>
        <notes><![CDATA[
        Test dependencies are not included in production builds and don't pose
        runtime security risks.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.junit\.jupiter/.*$</packageUrl>
        <cve>.*</cve>
    </suppress>
    
    <suppress>
        <notes><![CDATA[
        Mockito is a test-only dependency and doesn't pose runtime security risks.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.mockito/.*$</packageUrl>
        <cve>.*</cve>
    </suppress>
    
    <suppress>
        <notes><![CDATA[
        TestContainers is a test-only dependency and doesn't pose runtime security risks.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.testcontainers/.*$</packageUrl>
        <cve>.*</cve>
    </suppress>
    
    <!-- Suppress build-time only dependencies -->
    <suppress>
        <notes><![CDATA[
        Maven plugins and build-time dependencies don't pose runtime security risks.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.apache\.maven\.plugins/.*$</packageUrl>
        <cve>.*</cve>
    </suppress>
    
    <!-- Suppress specific false positives by filename -->
    <suppress>
        <notes><![CDATA[
        This is a false positive - the file is not the vulnerable component.
        ]]></notes>
        <filePath regex="true">.*\.jar</filePath>
        <cve>CVE-2020-8908</cve>
    </suppress>
    
</suppressions>